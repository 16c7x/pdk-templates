# Custom Template
#
<%- if params -%>
#
<%-   params.each do |param| -%>
# @param <%= param[:name] %> A description of this parameter.
<%-   end -%>
class <%= name %> (
<%-   params.each do |param| -%>
  <%= param[:type].ljust(max_type_length) %> $<%= param[:name] %>,
<%-   end -%>
) {
<%- else -%>
class <%= name %> {
<%- end -%>
}
